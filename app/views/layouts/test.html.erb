<style type="text/css">
#container {background-color:#00CCFF; border:1px dotted black; padding:1em; cursor:pointer;}
.modform {margin-bottom: 1em;}
.index {width:5em;}

.yui-skin-sam .yui-dt-liner { white-space:nowrap; }

#chart
	{
		width: 400px;
		height: 200px;

		margin-left: 0.9em;
	}

#datatable
{
  margin-left: 0.9em;
}
	.yui-dt-table {width: 500px;}

	.chart_title
	{
		display: block;
		font-size: 1.2em;
		font-weight: bold;

		margin-bottom: 0.4em;
	}

</style>

</head>
<body class="yui-skin-sam">

<div id="panel1"> </div>
<button id="show1">Show panel1</button>
<button id="hide1">Hide panel1</button>
<span class="chart_title">Bi-Charts</span>

</body>
