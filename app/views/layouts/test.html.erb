<link rel="stylesheet" type="text/css" href="http://yui.yahooapis.com/2.8.1/build/assets/skins/sam/resize.css"/>
<link rel="stylesheet" type="text/css" href="http://yui.yahooapis.com/2.8.1/build/assets/skins/sam/layout.css"/>


<!-- Dependency kaya -->

<%= javascript_include_tag 'yui/yahoo-min.js' %>
