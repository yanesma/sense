<script src="http://yui.yahooapis.com/2.8.1/build/yuitest/yuitest-min.js"></script>

<%= javascript_include_tag 'yui/datatable' %>

<%= javascript_include_tag 'container' %>

<%= javascript_include_tag 'prototype' %>
